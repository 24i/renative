  "scripts": {
    "clean": "git clean -dfx && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/haste-*",
    "postinstall": "npm run lerna && npx rnv init",
    "watch": "npx lerna run --parallel watch",
    "lerna": "npx lerna bootstrap",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:reset": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "ios": "rnv run -p ios",
    "tvos": "rnv run -p tvos",
    "tizen": "rnv run -p tizen -t T-samsung-5.0-x86",
    "android": "rnv run -p android",
    "androidtv": "rnv run -p androidtv",
    "androidwear": "rnv run -p androidwear",
    "web": "rnv run -p web",
    "webos": "rnv run -p webos -t emulator",
    "macos": "npx rnv run -p macos",
    "windows": "npx rnv run -p windows",
    "test": "jest",
    "preversion": "npm run test",
    "version": "./packages/rnv/utils/update-versions.sh",
    "doc": "doctoc ."
  },
  "dependencies": {
