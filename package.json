    ],
    "release-it": {
        "publish": "local",
        "skipRootPublish": true,
        "rootPublishCommand": "rnv deploy -p ios -s debug",
        "git": {
            "tagName": "v${version}",
            "requireCleanWorkingDir": false
        },
        "npm": {
            "publish": false
        },
        "hooks": {
            "before:git": "rnv pkg version ${version}"
        }
    }
