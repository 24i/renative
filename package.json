{
  "name": "react-native-vanilla",
  "version": "0.1.0",
  "codename": "Valhalla",
  "private": false,
  "description": "Most fundamental setup of react native project with latest Swift and Kotlin support",
  "author": "Pavel Jacko <i@pavjacko.com>",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "react-native run-ios --project-path ios",
    "fix:ios": "sed -i '' 's/#import <RCTAnimation\\/RCTValueAnimatedNode.h>/#import \"RCTValueAnimatedNode.h\"/' ./node_modules/react-native/Libraries/NativeAnimation/RCTNativeAnimatedNodesManager.h && sed -i '' 's/#import <fishhook\\/fishhook.h>/#import <React\\/fishhook.h>/' ./node_modules/react-native/Libraries/WebSocket/RCTReconnectingWebSocket.m",
    "setup": "npm run i && npm run fix:ios",
    "android": "cd android && ./gradlew appStart",
    "ios:update": "cd ios && pod update",
    "test": "jest"
  },
  "dependencies": {
    "jsc-android": "^216113.0.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-native": "0.56.0"
  },
  "devDependencies": {
    "babel-jest": "23.4.0",
    "babel-preset-react-native": "5.0.0",
    "jest": "23.4.1",
    "react-test-renderer": "16.4.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
