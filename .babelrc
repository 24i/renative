{
    "presets": ["next/babel"],
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["./"],
                "alias": {
                    "^react-native$": "react-native-web",
                    "react-native/Libraries/Renderer/shims/ReactNativePropRegistry": "react-native-web/dist/modules/ReactNativePropRegistry",
                    "react-native/Libraries/Image/resolveAssetSource": "react-native-web/dist/exports/Image/resolveAssetUri",
                    "react-native-vector-icons": "react-native-web-vector-icons",
                    "react-native-screens": "node_modules/react-native-screens",
                    "react-navigation-tabs": "node_modules/react-navigation-tabs",
                    "react-navigation-stack": "node_modules/react-navigation-stack",
                    "react-navigation": "react-navigation",
                    "@react-navigation": "node_modules/@react-navigation",
                    "react-native-gesture-handler": "node_modules/react-native-gesture-handler",
                    "react-native-reanimated": "node_modules/react-native-reanimated",
                    "react-native-camera": "node_modules/react-native-camera",
                    "react-native-actionsheet": "node_modules/react-native-actionsheet",
                    "react-native-root-toast": "node_modules/react-native-root-toast",
                    "react-native-root-siblings": "node_modules/react-native-root-siblings",
                    "static-container": "node_modules/static-container",
                    "react-native-material-dropdown": "node_modules/react-native-material-dropdown",
                    "react-native-material-buttons": "node_modules/react-native-material-buttons",
                    "react-native-material-textfield": "node_modules/react-native-material-textfield",
                    "react-native-material-ripple": "node_modules/react-native-material-ripple",
                    "react-native-easy-grid": "node_modules/react-native-easy-grid",
                    "native-base-shoutem-theme": "node_modules/native-base-shoutem-theme",
                    "react-native-drawer": "node_modules/react-native-drawer",
                    "react-native-safe-area-view": "node_modules/react-native-safe-area-view",
                    "react-native-keyboard-aware-scroll-view": "node_modules/react-native-keyboard-aware-scroll-view",
                    "query-string": "node_modules/query-string",
                    "split-on-first": "node_modules/split-on-first",
                    "strict-uri-encode": "node_modules/strict-uri-encode",
                    "@react-navigation/core": "node_modules/@react-navigation/core",
                    "@react-navigation/web": "node_modules/@react-navigation/web",
                    "react-native-animatable": "node_modules/react-native-animatable",
                    "react-native-collapse-view": "node_modules/react-native-collapse-view",
                    "react-native-color-picker": "node_modules/react-native-color-picker",
                    "react-native-ios-picker": "node_modules/react-native-ios-picker",
                    "react-native-modal-datetime-picker": "node_modules/react-native-modal-datetime-picker/",
                    "react-native-modal": "node_modules/react-native-modal",
                    "react-native-paper": "node_modules/react-native-paper",
                    "react-native-platform-touchable": "node_modules/react-native-platform-touchable",
                    "react-native-super-grid": "node_modules/react-native-super-grid",
                    "react-native-tab-view": "node_modules/react-native-tab-view",
                    "react-native-simple-markdown": "node_modules/react-native-simple-markdown",
                    "react-native-swipe-gestures": "node_modules/react-native-swipe-gestures",
                    "react-native-switch": "node_modules/react-native-switch",
                    "react-native-orientation-locker": "node_modules/react-native-orientation-locker",
                    "@react-navigation/native": "node_modules/@react-navigation/native"
                }
            }
        ],
            "@babel/plugin-proposal-export-default-from",
    ]
}
