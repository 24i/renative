{
    "id": "engine-rn-tvos",
    "engineExtension": "rnt",
    "overview": "React native based engine with added support for tvOS platform. Runs standard react native apps with native support",
    "plugins": {
        "react": "source:rnv",
        "react-art": "source:rnv",
        "react-dom": "source:rnv",
        "react-native": "npm:react-native-tvos@latest",
        "react-native-web": "source:rnv",
        "@react-native-community/cli-platform-ios": "source:rnv",
        "@react-native-community/cli": "source:rnv"
    },
    "npm": {
        "devDependencies": {}
    },
    "platforms": {
        "tvos": {
            "engine": "engine-rn-tvos",
            "npm": {
                "devDependencies": {
                    "xcode": "2.1.0"
                },
                "optionalDependencies": {
                    "ios-deploy": "1.10.0"
                }
            }
        }
    }
}