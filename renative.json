{
    "env": {
        "SOME_ENV_VAR": "xxxx"
    },
    "projectName": "renative",
    "globalProfile": "rnv",
    "isWrapper": true,
    "paths": {
        "globalConfigFolder": "~/.rnv",
        "appConfigsFolder": "./appConfigs",
        "platformTemplatesFolder": "RNV_HOME/platformTemplates",
        "entryFolder": "./",
        "platformAssetsFolder": "./platformAssets",
        "platformBuildsFolder": "./platformBuilds",
        "projectConfigFolder": "./projectConfig"
    },
    "defaults": {
        "ports": {
            "ios": "9091"
        },
        "template": "renative-template-hello-world",
        "supportedPlatforms": [
            "ios",
            "android",
            "androidtv",
            "androidwear",
            "web",
            "tizen",
            "tvos",
            "webos",
            "macos",
            "windows",
            "tizenwatch",
            "tizenmobile",
            "kaios",
            "firefoxos",
            "firefoxtv"
        ],
        "schemes": {
            "run": "debug",
            "build": "release",
            "export": "release"
        },
        "targets": {

        }
    },
    "currentTemplate": "renative-template-hello-world"
}
